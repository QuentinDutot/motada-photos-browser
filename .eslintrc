{
    "extends": "airbnb",
    "env": {
      "es6":     true,
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [0]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
}
